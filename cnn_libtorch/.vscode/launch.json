    "version": "0.2.0",
    "configurations": [
        {
            "name": "Conv Neural Network - cnn_classify",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cnn_classify",
            "args": ["./network.pt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++-7 build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
